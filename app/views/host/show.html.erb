<div>
	It works!
</div>
<div>
	<p>Room: <%= @host.room %></p>
	<p>Playlist <%= @host.playlist.key %></p>
</div>
<div>
	<%= form_tag @host.room + '/search/' do %>
		<%= label_tag :title %>
		<%= text_field_tag :title %>
	<% end %>
</div>
<div>
<% @host.playlist.songs.each do |song| %>
	<div>
		<%= song.title %>
	</div>
<% end %>
	<ul>
		<% @host.playlist.songs.each do |song| %>
			<li>
				<p>Song: <%= song.title %></p>
				<p>Artist: <%= song.artist %></p>
			</li>
		<% end %>
</div>
<div>
	<%= link_to 'Add song', host_path + '/search' %>
</div>
<div>
	<p>
		<%= @get %>
	</p>
</div>
