<div class="background"></div>

<div class="logo"></div>

<% @songs.each do |song| %>
	<%= form_tag '/' + @host.room + '/create/' do %>
		<div class="songChoice" style='cursor:pointer;'>
			<%= song['name'] %> - <%= song['artist'] %>
			<%= hidden_field_tag :trackKey, song['key'] %>
		</div>
	<% end %>
<% end %>
